<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:todo="clr-namespace:Todo;assembly=Todo"
             x:Class="Todo.App">
  <Application.Resources>
    <ResourceDictionary>
        
        
      <todo:CheckOnPositiveValueConverter x:Key="CheckOnPositiveValueConverter"></todo:CheckOnPositiveValueConverter>

            <todo:StringToUpperConverter x:Key="StringToUpperConverter"></todo:StringToUpperConverter>

            <Color x:Key="primaryGreen">#91CA47</Color>
      <Color x:Key="primaryDarkGreen">#6FA22E</Color>
      <Color x:Key="DeleteColor">#FFD61510</Color>
      <Color x:Key="BaseBlueColor">#60A0FF</Color>
			<Color x:Key="ExtraLightGrayColor">#F5F5F5</Color>
		</ResourceDictionary>
  </Application.Resources>
</Application>